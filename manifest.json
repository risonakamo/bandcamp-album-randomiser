{
    "name":"Bandcamp Album Randomiser",
    "short_name":"bcarandom",
    "description":"randomly open bandcamp album",
    "version":"1.0.0",
    "homepage_url":"https://github.com/risonakamo/bandcamp-album-randomiser",
    "manifest_version":3,

    "permissions":["tabs"],
    "host_permissions":["https://*.bandcamp.com/"],

    "content_scripts":[{
        "matches":["https://*.bandcamp.com/"],
        "js":["build/bcarandom.iife.js"]
    }]
}